{
    "compilerOptions": {
      "jsx": "react",
      "moduleResolution": "node",
      "target": "es5",
      "strict": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [
      "src/**/*"
    ]
  }
  